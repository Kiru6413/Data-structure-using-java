class Solution {
    public boolean isValid(String s) {
       int top=-1; 
       char stack[]=new char[s.length()];
       for(int i=0;i<s.length();i++){
        char ch=s.charAt(i);
        if(ch=='('||ch=='{'||ch=='['){
            stack[++top]=ch;
        }
        else{
            if(top<0)return false;
            char open=stack[top--];
            if((open=='('&&ch!=')')||(open=='['&&ch!=']')||(open=='{'&&ch!='}'))return false;
        }
       }
       return top==-1;
    }
}
