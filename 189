class Solution {
    public static void fun(int[] nums,int s,int e){
        while(s<e){
            int t=nums[s];
            nums[s]=nums[e];
            nums[e]=t;
            s++;
            e--;
        }
    }
    public void rotate(int[] nums, int k) {
        int l=nums.length;
        k=k%l;
        fun(nums,0,l-1);
        fun(nums,0,k-1);
        fun(nums,k,l-1);
    }
}
